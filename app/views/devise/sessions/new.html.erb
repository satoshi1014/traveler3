<div class="body">
  <%= image_tag "/wolf.jpg", class: 'video-content', style: "width:100%" %>
  <div class="login">
    <div class="login-space">
      <h2 class="sign-up-title1">
        <a href="/users/sign_up" class="sign-in-title-btn1">
          sign up
        </a>
      </h2>
      <h2 class="sign-in-title1">
          sign in
      </h2>


    <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "form" }) do |f| %>
      <div class="field">
        <%= f.label :email, {class: "sign-up-email"} %><br />
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "email" %>
      </div>

      <div class="field">
        <%= f.label :password, {class: "sign-up-password"} %><br />
        <%= f.password_field :password, autocomplete: "current-password", class: "password" %>
      </div>

      <% if devise_mapping.rememberable? %>
        <div class="field">
          <%= f.check_box :remember_me, class: "checkbox" %>
          <%= f.label :remember_me, class: "checkbox-name" %>
        </div>
      <% end %>

      <div class="actions">
        <%= f.submit "Log in", class: "sign-up-btn" %>
      </div>
    <% end %>

    <%# <%= render "devise/shared/links" %> 
    </div>
  </div>
</div>